#5
 for N in range(516):
     b = f'{N}'
     if N % 2 == 0:
         b += '10'
     else:
         b = '1' + b + '10'
     if int(b, 2) > 516:
         print(N)
         break

#6
from turtle import *
left(90)
for i in range(7):
    forward(300)
    right(120)
pu()
for x in range(1,9):
    for y in range(1,10):
        goto(x*30,y*30)
        dot(5)
done()

#14
n = 0
a = 3 * 16**2018 - 2 * 8**1028 - 3 * 4**1100 - 2**1050 - 2022
while a > 0:
    n += a % 4 == 3
    a = a // 4
print(n)
